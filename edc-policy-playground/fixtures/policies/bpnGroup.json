{
  "@context": [
    "http://www.w3.org/ns/odrl.jsonld",
    "https://w3id.org/tractusx/edc/v0.0.1",
    {
      "@vocab": "https://w3id.org/edc/v0.0.1/ns/"
    }
  ],
  "@type": "PolicyDefinition",
  "@id": "{{POLICY_ID}}",
  "policy": {
    "@type": "Set",
    "permission": [
      {
        "action": "use",
        "constraint": [
          {
            "leftOperand": "BusinessPartnerGroup",
            "operator": "isPartOf",
            "rightOperand": "<group>"
          }
        ]
      }
    ]
  }
}
